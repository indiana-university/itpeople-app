    "@types/traverse": "^0.6.32",
    "axios": "^0.18.0",
    "dom-storage": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "json-server": "0.14.0",
