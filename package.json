{
  "name": "itpeople-app",
  "version": "2019.9.0",
  "private": true,
  "license": "BSD-3-Clause",
  "dependencies": {
    "connected-react-router": "^6.7.0",
    "jwt-decode": "^2.2.0",
    "query-string": "^6.1.0",
    "react": "^16.12.0",
    "react-csv": "^1.1.1",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-form": "^8.1.0",
    "redux-saga": "^0.16.0",
    "rivet-collapsible": "^0.2.10",
    "rivet-react": "^1.4.0",
    "rivet-switch": "^0.3.1",
    "rivet-uits": "^1.2.1",
    "typesafe-actions": "^2.0.4"
  },
  "config": {
    "dbport": "3001"
  },
  "scripts": {
    "db": "json-server --watch src/db.json --routes json_server/db.routes.json --port $npm_package_config_dbport --middlewares ./json_server/db.middleware.auth.js ./json_server/db.middleware.idlookup.js ./json_server/db.middleware.routes.js",
    "db:admin": "yarn db --middlewares ./json_server/db.middleware.admin.js",
    "db:user": "yarn db --middlewares ./json_server/db.middleware.user.js",
    "db:chaos": "yarn db:admin --middlewares ./db.middleware.chaos.js",
    "db:slow": "yarn db:admin --middlewares ./json_server/db.middleware.slow.js",
    "db:switch": "yarn db --middlewares ./json_server/middleware.switcher.js",
    "dev": "server-test db:user $npm_package_config_dbport start",
    "dev:admin": "server-test db:admin $npm_package_config_dbport start",
    "dev:chaos": "server-test db:chaos $npm_package_config_dbport start",
    "dev:user": "server-test db:user $npm_package_config_dbport start",
    "dev:slow": "server-test db:slow $npm_package_config_dbport start",
    "dev:switch": "server-test db:switch $npm_package_config_dbport start",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest --env=jsdom --testRegex \"(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$\"",
    "pact": "CI=true jest --env=node --runInBand --testMatch \"**/*.pact.contract.ts\"",
    "eject": "react-scripts-ts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "server-test dev:switch 3000 cy:open",
    "ci": "server-test dev:switch 3000 cy:run"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@pact-foundation/pact": "^7.0.3",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/lowdb": "^1.0.6",
    "@types/node": "^10.9.4",
    "@types/react": "^16.9.19",
    "@types/react-csv": "^1.1.1",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-test-renderer": "^16.8.1",
    "@types/redux-form": "^7.4.8",
    "@types/traverse": "^0.6.32",
    "axios": "^0.18.0",
    "cypress": "3.3.1",
    "cypress-testing-library": "^2.3.6",
    "dom-storage": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest-dom": "^3.1.2",
    "jest-fetch-mock": "^2.1.1",
    "jest-localstorage-mock": "^2.4.0",
    "json-server": "0.14.0",
    "lowdb": "^1.0.0",
    "raf": "^3.4.1",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga-test-plan": "^3.7.0",
    "start-server-and-test": "^1.7.12",
    "traverse": "^0.6.6",
    "typescript": "^3.0.3"
  }
}
