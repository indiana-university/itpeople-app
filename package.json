  "scripts": {
    "json-server": "json-server --watch db.json --routes db.routes.json --port 3001",
    "dev": "yarn json-server & yarn start",
    "dev-admin": "yarn json-server -- --middlewares ./db.middleware.admin.js ./db.middleware.crud.js & yarn start",
    "dev-user": "yarn json-server -- --middlewares ./db.middleware.user.js ./db.middleware.crud.js & yarn start",
    "dev-slow": "yarn json-server -- --middlewares ./db.middleware.user.js ./db.middleware.slow.js ./db.middleware.crud.js & yarn start",
    "tdd": "yarn json-server & jest --env=jsdom --watch && kill 0",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "yarn json-server -- --middlewares ./db.middleware.crud.js & jest --env=jsdom && kill 0",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^7.0.3",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
