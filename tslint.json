{
  "extends": [ "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "no-unused-imports": true,    
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
