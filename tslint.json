{
  "extends": [ "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
